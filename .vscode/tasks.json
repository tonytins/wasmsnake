  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "cargo: web build",
      "command": "cargo",
      "args": ["web", "build"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "shell",
      "label": "cargo: web start",
      "command": "cargo",
      "args": ["web", "start"]
    },
    {
      "type": "shell",
      "label": "cargo: web deploy",
      "command": "cargo",
      "args": ["web", "deploy"]
    }
  ]
}
